{

  "manifest_version": 2,
  "name": "Sci-hub",
  "version": "0.1",

  "description": "Redirects to sci-hub from article web page.",

  "icons": {
    "48": "icon-48.jpeg",
    "96": "icon-96.jpeg"
  },

  "background": {
    "scripts": ["scihub.js"]
  },

 "browser_action": {
  "default_icon": {
    "19": "icon-19.png",
    "38": "icon-38.png"
  },
  "default_title": "Access sci-hub."
}

}
